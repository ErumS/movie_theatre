<!DOCTYPE html>
<html>  
  <head>
    <title>Auditorium</title>
    <link href="/assets/auditoria_manifest.css" media="all" rel="stylesheet" type="text/css" />
  </head>
  <body>
    <div id="index_audi">
      <h3>Listing Auditoria</h3>
      <div class="hidden">
        <table class="audi" cellpadding="10">
          <tr>
            <th>Screen Size</th>
            <th>Link</th>
          </tr>
          <tr id="template">
            <td id="screen_size"></td>
            <td id="show"></td>
            <td id="delete"></td>
          </tr>
        </table>
      </div>
    </div> 

    <div id="show_audi">
      <div class="hidden">
        <table id="show_template"" cellpadding="10">
          <tr>
            <td>Screen Size:</td> <td id="screen_size"></td>
          </tr>
          <tr>
            <td>No of Seats:</td> <td id="no_of_seats"></td>
          </tr>
          <tr>
            <td id="back"></td> 
          </tr>
        </table>
      </div>
    </div>
  </body>
</html>
