<!DOCTYPE html>
<html>  
  <head>
    <title>Ticket</title>
    <link href="/assets/tickets_manifest.css" media="all" rel="stylesheet" type="text/css" />
  </head>
  <body>
    <div id="index_tick">
      <h3>Listing tickets</h3>
      <div class="hidden">
        <table class="ticket" cellpadding="10">
          <tr>
            <th>PURCHASE</th>
            <th>Link</th>
          </tr>
          <tr id="template">
            <td id="purchase_date"></td>
            <td id="show"></td>
            <td id="delete"></td>
          </tr>
        </table>
      </div>
    </div> 

    <div id="show_tick">
      <div class="hidden">
        <table id="show_template"" cellpadding="10">
          <tr>
            <td>Purchase Date:</td> <td id="purchase_date"></td>
          </tr>
          <tr>
            <td>Movie Date:</td> <td id="movie_date"></td>
          </tr>
          <tr>
            <td>Price:</td> <td id="price"></td>
          </tr>
          <tr>
            <td id="back"></td> 
          </tr>
        </table>
      </div>
    </div>

  </body>
</html>
